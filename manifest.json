{
    "name": "K JSON Lint",
    "description": "JSON Lint Chrome Plugin - Pretty Print JSON instantly",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "storage",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "/images/KJsonLint.png"
        }
    },
    "icons": {
        "128": "/images/KJsonLint.png"
    }
}